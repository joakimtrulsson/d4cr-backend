    "react-icons": "^5.0.1",
    "react-media-library": "^2.0.3",
    "react-modal": "^3.16.1",
    "sass": "^1.70.0",
    "slate": "^0.101.5",
    "slate-react": "^0.101.5",
    "uuid": "^9.0.1"
